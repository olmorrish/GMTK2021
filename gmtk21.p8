--game logic

function next_level()
	level_num+=1
	
	--move player respawn
	player.resp_x=(level_num*128)%1024+8
	player.resp_y=(flr(level_num/8))*128+8
		
 
 respawn(false)
end

function respawn(was_death)
	
	if was_death then
		lives_left-=1
		if lives_left<1 then
		 game_over()
		end
	end 
	
	--reset the level
	player.x=player.resp_x
	player.y=player.resp_y
 player.dx=0
 player.dy=0
 
	carrying_egg=true
end

function game_over()
	gameover=true
end
