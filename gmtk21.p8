--update and draw

function _update()
	player_update()
	egg_update()
	camera_update()
end

function _draw()
 cls()
	map(0,0)
	player_draw()
	egg_draw()
	ui_draw()
	debug_draw()
end

function camera_update()
 cam_x=(level_num*128)%1024
 --local cam_y=0
 cam_y=(flr(level_num/8))*128
 
 camera(cam_x,cam_y)
end

function player_draw()
 
 if player.jumping then
 	player.sp=5
 
 elseif player.falling then
 	player.sp=6
 
 --idle no egg=head bob!
 elseif abs(player.dx)<0.1 
 and not carrying_egg then
	 if time()>player.next_anim_time then
	 	player.sp+=1
	 	if player.sp>4 then
	 	 player.sp=1
	 	end
	 	player.next_anim_time=time()+player.anim_speed
	 end 
	
	--moving (egg or no egg)
	elseif abs(player.dx)>0.1 then
	 if time()>player.next_anim_time then
	 	player.sp+=1
	 	if player.sp>3 then
	 	 player.sp=1
	 	end
	 	player.next_anim_time=time()+player.anim_speed
	 end 
	 
	--idle with egg
	else
	 player.sp=1
	end
 
 player.flp=player.dx<0
 
 spr(player.sp,player.x,
  player.y,1,1,player.flp)
 
end

function egg_draw()
	spr(egg.sp,egg.x,egg.y)
end

function ui_draw()
 spr(52,75,0)

 local str_lives="x"..lives_left
 print(str_lives,66,1,0)
 print(str_lives,67,2,7)

 local str_lvl="level "..level_num
 if(level_num<10) then
  print(str_lvl,86,1,0)
  print(str_lvl,87,2,7) 	
 else
  print(str_lvl,84,1,0)
  print(str_lvl,85,2,7)
 end
end

function debug_draw()
 --player coordinates
 local c="plr:".."("..flr(player.x)..","..flr(player.y)..")"
 print(c,cam_x+8,cam_y+8,8)
end
